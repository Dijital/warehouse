<table id="browser" cellspacing="0" cellpadding="0">
  <thead>
    <tr>
      <th>File name</th>
      <th>Location</th>
    </tr>
  </thead>
  <% @photos.each do |photo| -%>
  <tr>
    <% path = photo.path.split('/') %>
    <td class="item">
      <%= link_to h(path.last), "", :class => 'image' %>
    </td>
    <td><span class="log"><% path.pop %> <%= path.join('/') %></span></td>
  </tr>
  <% end -%>
</table>

<%= will_paginate @photos %>
