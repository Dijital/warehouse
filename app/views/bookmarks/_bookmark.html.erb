<li id="<%= dom_id bookmark %>">
  <% if repository_admin? -%><%= link_to_remote 'Delete', {:url => bookmark_path(bookmark), :with => "'_method=delete'", 
    :confirm => "Are you sure you want to delete this bookmark?"}, :class => 'trash list-del' %><% end -%>
  <%= link_to h(bookmark.label), browser_path(:paths => bookmark.paths), :title => h(bookmark.description) %>
</li>