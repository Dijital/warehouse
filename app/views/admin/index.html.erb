<h2>Repositories</h2>
<ul id="repos">
<% @repos.each do |repo| %>
  <li class="repo">
    <%=h repo.name %>
    <span class="log"><%= repo.path %></span>
    <ul class="admin-controls">
      <li class="edit"><a href="#" title="Edit">Edit</a></li>
      <li class="del"><a href="#" title="Delete">Delete</a></li>
    </ul>
  </li>
<% end %>